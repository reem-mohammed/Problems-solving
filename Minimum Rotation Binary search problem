package QI;

/**
 *
 * @author reemabdallah
 */
public class MinRotationBinary {
    public int findRotationNumber(int arr []){
        
        
        int left=0 , right=arr.length-1,rotationNumber;
        
        if(arr.length==1){
            rotationNumber=0;
            return rotationNumber;
        }
        
        if(arr[left]<arr[right]){
            rotationNumber=0;
            return rotationNumber;
        }
        
        
                
        
        
        return -1;
        
    }
    public static void main(String args[]){
       MinRotationBinary num=new MinRotationBinary();
       int [] arr = {8,9,10,2,5,6};
       num.findRotationNumber(arr);
    }
    
}
